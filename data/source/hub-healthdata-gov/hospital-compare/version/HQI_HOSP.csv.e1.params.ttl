   a foaf:OnlineAccount;
   foaf:accountName "jpm78";
   dcterms:isPartOf <http://purl.org/klab/lod/machines/schist#>;
   sioc:account_of  <http://tw.rpi.edu/instances/JamesMcCusker>;
